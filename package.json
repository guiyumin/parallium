{
  "name": "parallium",
  "version": "0.1.1",
  "description": "High-performance, local-first spreadsheet powered by WebGPU",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "spreadsheet",
    "webgpu",
    "gpu",
    "high-performance",
    "local-first",
    "data-analysis",
    "grid"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.18.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@webgpu/types": "^0.1.51"
  }
}
